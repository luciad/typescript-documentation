{
  "name": "l-td",
  "private": true,
  "description": "Luciad typescript documentation HTML generator",
  "version": "0.1.0",
  "license": "MIT",
  "main": "src/index.js",
  "bin": "l-td.sh",
  "scripts": {
    "build": "./l-td.sh",
    "develop": "./l-td.sh -d true",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "pretest": "./l-td.sh -m ./cypress/media -s ./cypress/snippets -c true ; npx typedoc --tsconfig ./cypress/src/tsconfig.json --json ./content/docu.json ./cypress/src",
    "test": "start-server-and-test serve http://localhost:8000 cy:open",
    "test:dev": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.4.2",
    "babel-jest": "^26.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-gatsby": "^0.4.12",
    "cypress": "^4.12.1",
    "gatsby": "^2.24.35",
    "gatsby-plugin-local-search": "^1.1.1",
    "gatsby-plugin-manifest": "^2.4.22",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.25",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-json": "^2.4.11",
    "html-react-parser": "^0.10.5",
    "prettier": "^1.19.1",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.13.1",
    "react-use-flexsearch": "^0.1.1",
    "sanitize-html": "^1.27.2",
    "start-server-and-test": "^1.11.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "typedoc": "^0.17.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luciad/typescript-documentation"
  },
  "bugs": {
    "url": "https://github.com/luciad/typescript-documentation/issues"
  }
}
